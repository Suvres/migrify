{
    "name": "migrify/config-format-converter",
    "description": "Convert Symfony XML format to YAML or clarify Symfony/Nette syntax to readable one - all the useful utils around configs.",
    "license": "MIT",
    "bin": "bin/config-format-converter",
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "nette/neon": "^3.0",
        "nette/utils": "^3.1",
        "symplify/package-builder": "^8.1",
        "symplify/autowire-array-parameter": "^8.1",
        "symplify/smart-file-system": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\ConfigFormatConverter\\": "src",
            "Migrify\\ConfigFormatConverter\\ConfigClarity\\": "packages/clarifier/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\ConfigFormatConverter\\Tests\\": "tests",
            "Migrify\\ConfigFormatConverter\\ConfigClarity\\Tests\\": "packages/clarifier/tests"

        }
    }
}
